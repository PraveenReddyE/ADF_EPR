{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-EPR"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlDatabase_linkedservice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase_linkedservice'"
		},
		"EPRlinkedservice_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'EPRlinkedservice'"
		},
		"SQL_TargetDB_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQL_TargetDB_LS'"
		},
		"AzureSqlDatabase_linkedservice_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "eprsqlservernew.database.windows.net"
		},
		"AzureSqlDatabase_linkedservice_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "eprdatabase"
		},
		"AzureSqlDatabase_linkedservice_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Praveene"
		},
		"EPRlinkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://epradlsgen2.dfs.core.windows.net"
		},
		"RestApi_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://reqres.in/api/users"
		},
		"SQL_TargetDB_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "eprsqlservernew.database.windows.net"
		},
		"SQL_TargetDB_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Target_DB"
		},
		"SQL_TargetDB_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Praveene"
		},
		"TumblingWindow_trigger_properties_pipeline_parameters_StartDate": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"TumblingWindow_trigger_properties_pipeline_parameters_EndDate": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-EPR_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureSqlDatabase_linkedservice_password": {
						"value": "[parameters('AzureSqlDatabase_linkedservice_password')]"
					},
					"EPRlinkedservice_accountKey": {
						"value": "[parameters('EPRlinkedservice_accountKey')]"
					},
					"SQL_TargetDB_LS_password": {
						"value": "[parameters('SQL_TargetDB_LS_password')]"
					},
					"AzureSqlDatabase_linkedservice_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase_linkedservice_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase_linkedservice_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase_linkedservice_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase_linkedservice_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase_linkedservice_properties_typeProperties_userName')]"
					},
					"EPRlinkedservice_properties_typeProperties_url": {
						"value": "[parameters('EPRlinkedservice_properties_typeProperties_url')]"
					},
					"RestApi_LS_properties_typeProperties_url": {
						"value": "[parameters('RestApi_LS_properties_typeProperties_url')]"
					},
					"SQL_TargetDB_LS_properties_typeProperties_server": {
						"value": "[parameters('SQL_TargetDB_LS_properties_typeProperties_server')]"
					},
					"SQL_TargetDB_LS_properties_typeProperties_database": {
						"value": "[parameters('SQL_TargetDB_LS_properties_typeProperties_database')]"
					},
					"SQL_TargetDB_LS_properties_typeProperties_userName": {
						"value": "[parameters('SQL_TargetDB_LS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADF-EPR_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-EPR_ArmTemplate_0')]"
			]
		},
		{
			"name": "ADF-EPR_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-EPR_ArmTemplate_1')]"
			]
		},
		{
			"name": "ADF-EPR_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-EPR_ArmTemplate_2')]"
			]
		},
		{
			"name": "ADF-EPR_ArmTemplate_4",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_4.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"TumblingWindow_trigger_properties_pipeline_parameters_StartDate": {
						"value": "[parameters('TumblingWindow_trigger_properties_pipeline_parameters_StartDate')]"
					},
					"TumblingWindow_trigger_properties_pipeline_parameters_EndDate": {
						"value": "[parameters('TumblingWindow_trigger_properties_pipeline_parameters_EndDate')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-EPR_ArmTemplate_3')]"
			]
		}
	]
}